{
  "manifest_version": 3,
  "name": "Fishix - Phishing Detector",
  "version": "1.0",
  "description": "Detects phishing websites using ML, rules, and VirusTotal.",
  "permissions": ["webRequest", "webRequestBlocking", "storage", "scripting"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon128.png",
      "48": "icon128.png",
      "128": "icon128.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["warning.html"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icon128.png",
    "48": "icon128.png",
    "128": "icon128.png"
  }
}

